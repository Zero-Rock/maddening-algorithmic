    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{ts,json,md}": "prettier --write"
  },
  "dependencies": {
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@vitest/coverage-istanbul": "^0.34.1",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.1",
    "ts-lint": "^4.5.1",
    "ts-node": "^10.9.1",
    "vite": "^6.0.12",
    "vitest": "^2.1.6"
